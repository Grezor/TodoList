{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "import",
        "node",
        "promise"
      ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "error"
    }
}
